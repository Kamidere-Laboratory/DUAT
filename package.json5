{
  name: "name",
  version: "1.0.0",
  description: "",
  main: "index.js",
  scripts: {

    new: "yarn add -D \
    @types/debug \
    @types/node \
    @typescript-eslint/eslint-plugin \
    @typescript-eslint/parser \
    debug \
    eslint \
    eslint-config-airbnb-base \
    eslint-plugin-import \
    eslint-scope \
    prettier \
    prettier-eslint \
    typescript \
    ts-node-dev \
    dotenv \
    @commitlint/config-conventional \
    @commitlint/cli",
    
    genPackageJson: "json5 -s 2 -o package.json package.json5",
    prebuild: "eslint --fix-dry-run --fix-type \"problem, suggestion, layout\" --max-warnings 0 --color --cache src/**/*",
    build: "tsc",
    start: "node ./dist/index.js",
    lint: "eslint --fix-dry-run --fix-type \"problem, suggestion, layout\" --max-warnings 0 --color --cache src/**/*",
    debug: 'ts-node-dev .'
  },
  husky: {
    hooks: {
      commit-msg: "commitlint -E HUSKY_GIT_PARAMS",
      pre-commit: "yarn lint"
    }
  },
  author: {
    name: "Kamide.re Laboratory",
    url: "https://gitlab.com/kamidere-laboratory"
  },
  contributors: [
    {
      name: "Robert \"RunAge\" Gajda",
      url: "https://gitlab.com/RunAge"
    }
  ],
  license: "MIT"
}
